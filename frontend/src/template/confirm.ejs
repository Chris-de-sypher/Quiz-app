<!-- @format -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Do you wish to continue?</title>

    <style>
      body {
        background-color: rgba(61, 34, 8, 0.89);
        display: flex;
        justify-content: center;
        place-items: center;
        padding: 0;
        margin: 0;
        height: 100vh;
      }
      div.container {
        background-color: green;
        padding: 10px;
        background-color: rgba(80, 41, 6, 0.89);
        border-radius: 6px;
        margin: 40px;
      }
      h1 {
        color: #eff;
        font-family: "Franklin Gothic Medium", "Arial Narrow", Arial, sans-serif;
      }
      button{
        padding: 9px 14px;
        background: rgb(9, 100, 9);
        color: #fff;
        font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        cursor: pointer;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <h1>
        You've reached the total number of question, now you will be redirected.
      </h1>
      <button type="button" onclick="redirect()">continue</button>
    </div>

    <script>
      function redirect(){
        window.location.href = '/api/v1/pages/dashboard';
        window.close();
      }
    </script>
  </body>
</html>
